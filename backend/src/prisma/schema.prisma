
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Leads {
  id String @id @default(uuid())
  nome String
  data_nascimento DateTime
  genero String
  nacionalidade String
  data_criacao DateTime @default(now())
  data_atualizacao DateTime @default(now())
  @@map("leads")
}

